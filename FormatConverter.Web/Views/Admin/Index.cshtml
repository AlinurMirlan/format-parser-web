@model IEnumerable<AdminActionVm>
@{
    ViewData["Title"] = "Admin Page";
}

<div class="container mt-4">
    <div class="card">
        <div class="card-body text-center">
            <h2 class="mt-3 mb-4">Admin Panel</h2>

            @foreach (var adminAction in Model)
            {
                <div class="mb-3">
                    <partial name="_AdminAction" model="adminAction" />
                </div>
            }
        </div>
    </div>
</div>
